  <SQLQuery>
    <![CDATA[SELECT EMPHOMEOFFICEWEEKSCEO.D1, 
       EMPHOMEOFFICEWEEKSCEO.D2, 
       EMPHOMEOFFICEWEEKSCEO.D3, 
       EMPHOMEOFFICEWEEKSCEO.D4, 
       EMPHOMEOFFICEWEEKSCEO.D5, 
       EMPHOMEOFFICEWEEKSCEO.D6, 
       EMPHOMEOFFICEWEEKSCEO.D7,                
       EMPHOMEOFFICEWEEKSCEO.EMP_NO,  
       EMPHOMEOFFICEWEEKSCEO.WEEK_ID, 
       EMPHOMEOFFICEWEEKSCEO.REMARKS,
       EMPHOMEOFFICEWEEKSCEO.HEAD_APPROVED,
    EMPHOMEOFFICEWEEKSCEO.SEND_MAIL,
       COE.FULL_NAME,
       COE.GRADE_ID ,
       COE.POSITION,
       COE.SSC,
       COE.SECTION,
       COE.DEPT,
       COE.ORG,
       COE.EMAIL_ADDRESS,
         COE.HOD_VALUE,
COE.EMPLOYMENT_CATEGORY,
COE.PEOPLE_GROUP_ID
FROM XX_EMP_HOME_OFFICE_WEEK_SC EMPHOMEOFFICEWEEKSCEO ,    XX_HOME_OFFICE_WEEK_LIST HWL,  APPS.CO_EMPLOYEE COE
WHERE EMPHOMEOFFICEWEEKSCEO.EMP_NO = COE.EMP_NO
AND HWL.WEEK_ID = EMPHOMEOFFICEWEEKSCEO.WEEK_ID
AND COE.EMPLOYMENT_CATEGORY = :p_employment_catagory 
--AND ( CASE WHEN :p_resp_id = 52870 THEN 1 
--ELSE COE.PEOPLE_GROUP_ID END) = :p_people_group_id
AND   HWL.END_DATE BETWEEN EFFECTIVE_START_DATE AND EFFECTIVE_END_DATE
ORDER BY  COE.GRADE_ID  , EMPHOMEOFFICEWEEKSCEO.EMP_NO , EMPHOMEOFFICEWEEKSCEO.WEEK_ID]]>
  </SQLQuery>
